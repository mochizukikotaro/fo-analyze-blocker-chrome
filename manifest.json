{
  "manifest_version": 2,
  "name": "fO AnalyzeBlocker",
  "description": "ferretOne のアクセス解析を無効にします",
  "version": "0.0.2",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon.png"
  },
  "permissions": [
    "*://*/*/",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "browser_action": {
    "name": "toggle extension"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}