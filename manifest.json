{
  "manifest_version": 2,
  "name": "fO AnalyzeBlocker",
  "version": "0.0.2",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "*://*/*/",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "browser_action": {
    "name": "toggle extension"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}